                    Content="Familie" 
                     
                    Click="HovedMenuFamilie_Click"
                    />

            <Button x:Name="HovedMenuVaccinationsKort" 
                    Style="{StaticResource MainPageKnap}"
                    Content="Vaccinekort" 
                   ></Button>

        </StackPanel>


    </Grid>
</Page>
